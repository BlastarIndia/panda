# First tests

def print(arg):
    console.log(arg)
end

def test(name, a, b):
    print(name + " passed:\n\t\t\t" + (a == b))
end

# Basic arithmetic and primitives

test("arithmetic", 4 + 4 * (5 - 22) + 5.3 / 2, -61.35)

test("booleans 1", (false or true) and false == false, true)
test("booleans 2", false and true == false, false)

test(">=", 3 >= 5, false)
test("<=", 3 <= 5, true)

test(">", 3 > 5, false)
test("<", 3 < 5, true)

test("not 1", not true, false)
test("not 2", not false, true)

var a = 5
test("unary minus", -a, 5 - 10)

# conditions
var a = 5
var b = true
if a == 5:
    b = false
end

test("if 1", b, false)

var a = 5
var b = true
if a == 5: b = false end

test("if 2", b, false)

var a = 5
var b = false
if a == 6:
    b = false
else:
    b = true
end

test("else 1", b, true)

var a = 5
var b = 0
if a == 1:
    b = 1
else if a == 2:
    b = 2
else if a == 3:
    b = 3
else:
    b = 4
end

test("else/if 1", b, 4)