# if/else

var a = 5
var b = true
if a == 5:
    b = false
end

test("if 1", b, false)

a = 5
b = true
if a == 5: b = false end

test("if 2", b, false)

a = 5
b = false
if a == 6:
    b = false
else:
    b = true
end

test("else 1", b, true)

a = 5
b = 0
if a == 1:
    b = 1
else if a == 2:
    b = 2
else if a == 3:
    b = 3
else:
    b = 4
end

test("else/if 1", b, 4)